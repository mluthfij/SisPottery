<%# <%= turbo_frame_tag "cart" do %>

<div class="alert alert-success m-4" role="alert">
<h1><i class="bi bi-cart"></i></h1>
    <% if @cart.orderables.count > 0 %>
        <%= render "cart/orderables" %>
        <%# <p> %>
        <%# Total: <%= @cart.total %>
        <%# </p> %>
    <% else %>
        Your cart is empty
    <% end %>
    <%= @cart.inspect %>
    <hr>
    <%= @orderable.inspect %>
</div>

<%# <% end %>