<div class="row row-cols-1 row-cols-md-3 g-4 mb-4">
    <% obj.order(id: :desc).each do |product| %>
        <div class="col">
            <div class="card h-100">
            <%#  %>
            <%# <div class="position-absolute top-0 end-0 m-1">

                <div class="dropdown dropstart">
                </div>            

            </div> %>
            <%#  %>
                <%= link_to product_path(product) do %>
                    <img src="https://img.freepik.com/free-photo/woman-showing-pinterest-icon_53876-40589.jpg?w=2000" class="card-img-top" alt="...">
                <% end %>
                <div class="card-body">
                    <h5 class="card-title"><%= link_to product.name, product, class:"text-decoration-none" %></h5>
                    <h6 class="card-subtitle mb-2 text-muted">Rp. <%= product.price %></h6>
                </div>
            </div>
        </div>
    <% end %>
</div>


